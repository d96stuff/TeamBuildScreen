<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="TeamBuildScreenSaver.Views.PartiallySucceededIcon"
	x:Name="UserControl"
	Width="256" Height="256" xmlns:BuildStatus="clr-namespace:TeamBuildScreenSaver.Views">

	<Grid x:Name="LayoutRoot">
		<Viewbox Stretch="Fill">
			<Grid Width="256" Height="256">
				<BuildStatus:FailedIcon HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="171" Height="171" Margin="85,85,0,0"/>
				<BuildStatus:SuccessIcon HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="171" Height="171" Margin="0,0,85,85"/>
			</Grid>
		</Viewbox>
	</Grid>
</UserControl>